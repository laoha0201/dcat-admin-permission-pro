{
    "name": "dcat-admin-permission-pro",
    "alias": "高级权限管理",
    "description": "快捷设置权限且一键导入权限列表",
    "type": "library",
    "keywords": ["dcat-admin", "extension"],
    "homepage": "https://github.com/nicesome/dcat-admin-permission-pro",
    "license": "MIT",
    "authors": [
        {
            "name": "nicesome",
            "email": "zhiyuan.me@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "dcat/laravel-admin": "~2.0",
        "doctrine/annotations": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "DcatAdmin\\PermissionPlus\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "DcatAdmin\\PermissionPro\\PermissionProServiceProvider",
        "laravel": {
            "providers": [
                "DcatAdmin\\PermissionPro\\PermissionProServiceProvider"
            ]
        }
    }
}
